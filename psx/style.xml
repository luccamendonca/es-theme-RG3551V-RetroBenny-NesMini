<theme>
	<formatVersion>3</formatVersion>
	<view name="system">
	</view>
	<feature supported="carousel">
		<view name="system">
			<image name="carousel_bar" extra="true">
				<color>ffc428</color>
			</image>
		</view>
	</feature>
	<view name="basic, detailed, video">

		<textlist name="gamelist">
			<selectorColor>ffc428</selectorColor>
			<selectedColor>000000</selectedColor>
			<primaryColor>ffc428</primaryColor>
			<secondaryColor>ffffff</secondaryColor>
		</textlist>
		<image name="player_box" extra="true">
			<color>ffc428</color>
		</image>
		<text name="publisher_label, developer_label, genre_label, rating_label, played_label"
			extra="true">
			<color>ffc428</color>
		</text>
	</view>
</theme>