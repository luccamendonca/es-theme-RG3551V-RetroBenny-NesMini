<theme>
	<formatVersion>3</formatVersion>


	<view name="system">


	</view>

	<feature supported="carousel">

		<view name="system">


			<image name="carousel_bar" extra="true">
				<color>cecece</color>
			</image>

		</view>

	</feature>


	<view name="basic, detailed, video">


		<textlist name="gamelist">
			<selectorColor>ee141f</selectorColor>
			<selectedColor>e7e7e7</selectedColor>
			<primaryColor>c8c8c8</primaryColor>
			<secondaryColor>4b4b4b</secondaryColor>
		</textlist>


		<image name="player_box" extra="true">
			<color>00aff1</color>
		</image>

		<text name="publisher_label, developer_label, genre_label, rating_label, played_label"
			extra="true">
			<color>c8c8c8</color>
		</text>

		<text
			name="md_lbl_rating, md_lbl_releasedate, md_lbl_developer, md_lbl_publisher, md_lbl_genre, md_lbl_players, md_lbl_lastplayed, md_lbl_playcount">
			<color>c8c8c8</color>
		</text>

		<text name="md_rating, md_developer, md_publisher, md_genre, md_players">
			<color>fef100</color>
		</text>


		<text name="md_players">
			<color>ffffff</color>
		</text>


	</view>

</theme>