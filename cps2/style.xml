<theme>
	<formatVersion>3</formatVersion>


	<view name="system">


	</view>

	<feature supported="carousel">

		<view name="system">


			<image name="carousel_bar" extra="true">
				<color>f1f000</color>
			</image>

		</view>

	</feature>

	<view name="basic, detailed, video">


		<textlist name="gamelist">
			<selectorColor>f1f000</selectorColor>
			<selectedColor>000000</selectedColor>
			<primaryColor>ffffff</primaryColor>
			<secondaryColor>0000c0</secondaryColor>
		</textlist>


		<image name="player_box" extra="true">
			<color>358dc0</color>
		</image>

		<text name="publisher_label, developer_label, genre_label, rating_label, played_label"
			extra="true">
			<color>358dc0</color>
		</text>


	</view>

</theme>