<theme>
	<formatVersion>3</formatVersion>
	<view name="system">
	</view>
	<feature supported="carousel">
		<view name="system">
		</view>
	</feature>
	<view name="basic, detailed, video">

		<textlist name="gamelist">
			<selectorColor>1f00cc</selectorColor>
			<selectedColor>ffffff</selectedColor>
			<primaryColor>1f00cc</primaryColor>
			<secondaryColor>1f00cc</secondaryColor>
		</textlist>
		<image name="player_box" extra="true">
			<color>1f00cc</color>
		</image>
		<text name="publisher_label, developer_label, genre_label, rating_label, played_label"
			extra="true">
			<color>ffffff</color>
		</text>

	</view>
</theme>