<theme>
	<formatVersion>3</formatVersion>
	<view name="system">
	</view>
	<feature supported="carousel">
		<view name="system">
			<image name="carousel_bar" extra="true">
				<color>fa0300</color>
			</image>
		</view>
	</feature>
	<view name="basic, detailed, video">
		<rating name="md_rating">
			<filledPath>./../art/heart_filled.png</filledPath>
			<unfilledPath>./../art/heart_empty.png</unfilledPath>
		</rating>
		<textlist name="gamelist">
			<selectorColor>fa0300</selectorColor>
			<selectedColor>ffffff</selectedColor>
			<primaryColor>000000</primaryColor>
			<secondaryColor>fa0300</secondaryColor>
		</textlist>
		<image name="player_box" extra="true">
			<color>fa0300</color>
		</image>
		<text name="publisher_label, developer_label, genre_label, rating_label, played_label"
			extra="true">
			<color>fa0300</color>
		</text>
		<datetime name="md_releasedate">
			<color>fa0300</color>
		</datetime>
	</view>
</theme>